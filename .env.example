# Optional API keys and settings for Rugby Planner
# Copy to .env and fill as needed

# Weather (Google Maps Weather + Places)
GOOGLE_MAPS_API_KEY=

# Pitchero (club calendar)
PITCHERO_CLUB_ID=7732
PITCHERO_TEAM_NAME=Ramsey (IoM)

# OpenAI for LLM-generated recovery plans
OPENAI_API_KEY=
# Optional model override (defaults to gpt-4o-mini if not set)
OPENAI_MODEL=gpt-4o-mini

# LangSmith (programmatic + UI evaluations)
LANGSMITH_API_KEY=
LANGSMITH_PROJECT=rugby-planner-demo
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
# Optional dataset name for programmatic evals
LANGSMITH_DATASET=rugby-planner-dataset
